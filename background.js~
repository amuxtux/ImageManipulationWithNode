var gm = require('gm')
  , dir = __dirname + '/imgs'

gm(dir + "/original.jpg")
  .background("#FF0000")  //red background
  .extent(340,300)
  .write(dir + '/background.jpg', function (err) {
    if (err) return console.dir(arguments)
    console.log(this.outname + " created  ::  " + arguments[3])
});

